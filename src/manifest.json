{
	"name": "Zabbix Watcher",
	"version": "1.7.4",
	"manifest_version": 2,
	"description": "Watcher for Zabbix.",

	"icons": {
		"16": "images/icon16.png",
		"48": "images/icon48.png",
		"128": "images/icon128.png"
	},

	"browser_action": {
		"default_icon": "images/icon_error.png",
		"default_popup": "popup.html"
	},

	"background": {
		"page": "background.html"
	},

	"permissions": [
		"notifications",
		"http://*/",
		"https://*/"
	]
}
